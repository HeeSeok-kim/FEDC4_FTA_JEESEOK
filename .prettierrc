{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^react(.*)",
		"<THIRD_PARTY_MODULES>",
    "^~/routes/(.*)$",
		"^~/pages/(.*)$",
		"^~/components/(.*)$",
		"^~/api/(.*)$",
		"^~/hooks/(.*)$",
		"^~/types/(.*)$",
		"^~/constants/(.*)$",
		"^[./]"
	],
	"importOrderSortSpecifiers": true
}
